.profile-in
  %header
    = image_tag 'magma-2.svg', class: 'logo-magma'
  .line
  %h4= t('profiles.profile')
  .pic
    = image_tag 'github.svg'
  .company
    = current_user.profile.company
  .location
    Location
  .email
    = current_user.email
  .complete
    - if current_user.should_create_profile?
      = link_to t('prodiles.complete'), edit_profile_path
  .cfp-logo
    = image_tag 'cfp-2.svg'
