- content_for :header_title do
  Are you ready to speak?
.eight-columns
  = form_for @proposal do |form|
    %fieldset.new-proposal
      = render 'form', :form => form

    .btn-aro.upper
      = form.submit 'Create', class: 'aro'

.one-column.days-left
  %p= t('devise.proposals.num')
  %span= t('devise.proposals.days')
